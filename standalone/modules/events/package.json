        "typescript": "^4.7.4",
        "rxjs": "^7.5.2"
      },
      "peerDependencies": {
        "@nestjs/core": "^7.0.0 || ^8.0.0 || ^9.0.0",
        "reflect-metadata": "^0.1.13"
      }
  }
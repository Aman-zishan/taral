        "reflect-metadata": "^0.1.13",
        "typescript": "^4.3.4"
      },
      "dependencies": {
        "aws-sdk": "^2.1137.0",
        "file-type": "^17.1.1",
        "fs-extra": "^10.0.0",
        "rxjs": "^7.5.2"
      },
      "peerDependencies": {
        "@nestjs/common": "^6.7.0 || ^7.0.0 || ^8.0.0",
        "@nestjs/core": "^6.7.0 || ^7.0.0 || ^8.0.0"
      }
